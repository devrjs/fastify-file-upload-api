{
  "name": "fastify-file-upload-api",
  "version": "1.0.0",
  "description": "API Node.js com Fastify, TypeScript e Swagger para upload de imagens",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "fastify",
    "typescript",
    "swagger",
    "upload",
    "images"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "fastify": "^5.3.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/uuid": "^10.0.0",
    "pino-pretty": "^13.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
