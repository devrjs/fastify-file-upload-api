{
  "name": "fastify-file-upload-api",
  "version": "1.0.0",
  "description": "API Node.js com Fastify, TypeScript e Swagger para upload de imagens",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "fastify",
    "typescript",
    "swagger",
    "upload",
    "images"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "dependencies": {
    "@fastify/multipart": "^8.0.0",
    "@fastify/static": "^7.0.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "fastify": "^4.24.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.5",
    "pino-pretty": "^10.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
